{
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=mydb;Username=postgres;Password=123456"
  },
  "Jwt": {
    "Key": "your-super-secret-key-with-at-least-32-characters",
    "Issuer": "xxx",
    "Audience": "xxx",
    "ExpiryInMinutes": 60
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log-.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 7
        }
      }
    ]
  },
  "SeedAdmin": {
    "Email": "xxx",
    "Password": "xxx"
  },
  "Mailtrap": {
    "Username": "xxx",
    "Password": "xxx"
  },
  "CloudinarySettings": {
    "CloudName": "xxx",
    "ApiKey": "xxx",
    "ApiSecret": "xx"
  },
  "SeatsIo": {
    "SecretKey": "xxx"
  },
  "AllowedHosts": "*"
}
